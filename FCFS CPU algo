#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() 
{
   int t1,t2,t3,t4;
    int sum1,sum2,sum3,sum4;
    
    scanf("%d",&t1);
    scanf("%d",&t2);
    scanf("%d",&t3);
    scanf("%d",&t4);
    sum1=0;
    sum2=t1;
    sum3=t1+t2;
    sum4=t1+t2+t3;
    printf("P1 (WT=%d), P2 (WT=%d), P3 (WT=%d), P4 (WT=%d)",sum1,sum2,sum3,sum4);
        
    return 0;
}
